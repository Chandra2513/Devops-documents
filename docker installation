docker images <-- to list all existed images in the local docker rigstry

docker search <-- this command used for to search the image/s in the docker hub

docker pull <-- this command used for to pull/get the image from the docker hub






 DOCKER INSTALLATION STEPS
 ==========================================================

hostnamectl set-hostname docker
exec bash
 yum install yum-utils device-mapper-persistent-data lvm2 -y
     yum-config-manager --add-repo  https://download.docker.com/linux/centos/docker-ce.repo
      ll /etc/yum.repos.d/
    yum install docker-ce -y
     docker version
     rpm -qi docker-ce
     systemctl status docker
     systemctl start docker
     systemctl status docker
     systemctl enable docker
     docker info
     docker images
     docker pull centos
     docker ps -a
    docker run -i -t centos /bin/bash
     docker search ubuntu
     docker search hello-world
     docker images

